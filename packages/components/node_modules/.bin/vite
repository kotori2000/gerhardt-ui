#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Documents/self-project/组件库/gerhardt-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/admin/Documents/self-project/组件库/gerhardt-ui/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/artifactory.intra.xiaojukeji.com+vite@3.1.7/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/artifactory.intra.xiaojukeji.com+vite@3.1.7/node_modules/vite/bin/vite.js" "$@"
fi
